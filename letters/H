#include <stdio.h>

int main()
{
  int i,j,k,t,spc;
    for(i=1;i<=6;i++)
    {
      for(j=1;j<=3;j++)
      {
    
      if(j==1||j==3||i==3||i==4)
      printf("2022");
      
      else 
      printf("\t");
    }
    printf("\n");
      }
}
