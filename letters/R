#include <stdio.h>
int main()
{
   int i,j,t=1,k,spc=2;
 for(i=1;i<=6;i++)
   {
         for(j=1;j<=3;j++)
   {
     if(i==1||j==1||i==5||i==6||((i==3||i==4||i==2)&&j==3)||(i==2&&j==2))
     printf("2022");
     else
     printf("\t");
   }
   printf("\n");

}
i==0;j==0;
 for(i=1;i<=6;i++)
   {
         for(j=1;j<=3;j++)
   {
     if(j==1)
     printf("2022");
    
   }
   for(k=5;k>=spc;k--)
   {
   printf(" ");
}
