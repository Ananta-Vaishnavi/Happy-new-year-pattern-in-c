#include <stdio.h>

int main()
{
	int i=0,j=0,k=0,spc=0;
   for(i=1;i<=10;i++)
   {
         for(j=1;j<=3;j++)
   {
     if(i==1||j==1||i==5||i==6||((i==3||i==4||i==2)&&j==3)||(i==2&&j==2))
     printf("2022");
     else
     printf("\t");
   }
   printf("\n");

}}
 
